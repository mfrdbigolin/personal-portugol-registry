algoritmo "Quantos anos você tem? EXERCICIOS 01 - 06"
var
   anos, ano_de_nasc, ano_atual: Inteiro
inicio

   Escreval ("Em Que Ano Você Nasceu?")
   Leia (ano_de_nasc)
   Escreval ("Em Que Ano Estamos?")
   Leia (ano_atual)
   
   anos <- ano_atual - ano_de_nasc
   
   Escreva ("Você Tem", anos, " Anos")
fimalgoritmo

